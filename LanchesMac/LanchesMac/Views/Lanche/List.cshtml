@* @model IEnumerable<LanchesMac.Models.Lanche> *@
@model LancheListViewModel


<div>
    <h2>@Model.CategoriaAtual</h2>
</div>

<div class="container">
    <div class="row">
        @foreach (var lanche in Model.Lanches)
        {
            @* o @lanche abaixo vem do foreach *@
            <partial name="_LanchesResumo" for="@lanche"/> 
        }
    </div>
</div>

